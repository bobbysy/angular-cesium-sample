{
  "name": "helloworld",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "eslint": "eslint --fix -c ts.eslintrc --ext=ts src/app -f html -o ./eslint/eslintResult.html",
    "build-deploy-gh-pages": "ng build --prod --base-href 'https://bobbysy.github.io/angular-cesium-sample/' && npx ngh --dir dist/angular-cesium-sample"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "npx eslint --fix -c ts.eslintrc --ext=ts src/app -f html -o ./eslint/eslintResult.html",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
