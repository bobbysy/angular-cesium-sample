{
  "name": "helloworld",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "ng e2e",
    "eslint": "eslint --fix -c .eslintrc.json --ext=ts src/app -f html -o ./eslint/eslintResult.html",
    "format": "prettier --write src/app/**/*",
    "build-deploy-gh-pages": "ng build --prod --base-href 'https://bobbysy.github.io/angular-cesium-sample/' && npx ngh --dir dist/angular-cesium-sample"
  },
  "jest": {
    "name": "helloworld",
    "preset": "jest-preset-angular",
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformer": [
          "<rootDir>/node_modules/jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.7",
    "@angular/material": "^9.1.3",
    "@angular/material-moment-adapter": "^9.1.3",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "@types/cesium": "^1.66.8",
    "angular-cesium": "0.0.65",
    "angular-cli-ghpages": "^0.6.2",
    "bootstrap": "^4.4.1",
    "cesium": "^1.67.0",
    "core-js": "^3.6.4",
    "jest-preset-angular": "^8.0.0",
    "moment": "^2.24.0",
    "npm": "^6.14.3",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.6",
    "@types/jest": "^25.1.4",
    "@types/node": "~13.9.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.1.0",
    "codelyzer": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "2.0.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.1",
    "typescript": "~3.7.5"
  },
  "author": "sy",
  "license": "ISC"
}
